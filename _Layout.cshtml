    <script>
        // Function to handle active class based on URL
        document.addEventListener("DOMContentLoaded", function () {
            // Get the current URL path (e.g. "/shop.cshtml")
            const currentPath = window.location.pathname;

            // Remove 'active' class from all nav-links and dropdown-items
            document.querySelectorAll('.navbar-nav .nav-link, .dropdown-item').forEach(link => link.classList.remove('active'));

            // Handle main nav-links
            if (currentPath.includes('shop.cshtml') && !currentPath.includes('shop-detail.cshtml')) {
                document.getElementById('shopLink').classList.add('active');
            } else if (currentPath.includes('shop-detail.cshtml')) {
                document.getElementById('shopDetailLink').classList.add('active');
            } else if (currentPath.includes('contact.cshtml')) {
                document.getElementById('contactLink').classList.add('active');
            } else if (currentPath.includes('index.cshtml')) {
                document.getElementById('homeLink').classList.add('active');
            }

            // Handle dropdown items
            if (currentPath.includes('cart.cshtml')) {
                document.getElementById('cartLink').classList.add('active');
            } else if (currentPath.includes('checkout.cshtml')) {
                document.getElementById('checkoutLink').classList.add('active');
            } else if (currentPath.includes('testimonial.cshtml')) {
                document.getElementById('testimonialLink').classList.add('active');
            } else if (currentPath.includes('404.cshtml')) {
                document.getElementById('404Link').classList.add('active');
            }

            // Optional: Click event listener for additional control
            document.querySelectorAll('.navbar-nav .nav-link, .dropdown-item').forEach(link => {
                link.addEventListener('click', function () {
                    document.querySelectorAll('.navbar-nav .nav-link, .dropdown-item').forEach(link => link.classList.remove('active'));
                    this.classList.add('active');
                });
            });
        });
    </script>




